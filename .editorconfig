# https://editorconfig.org/

root = true

[*]
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

[*.dart]
max_line_length = 80
indent_size = 2

[*.json]
indent_size = 2
insert_final_newline = ignore

[Makefile]
indent_style = tab
indent_size = 8
